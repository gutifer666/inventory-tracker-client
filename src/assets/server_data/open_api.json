    "components": {
        "schemas": {
            "User": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Identificador único del usuario."
                    },
                    "username": {
                        "type": "string",
                        "description": "Nombre del usuario.",
                        "maxLength": 50,
                        "minLength": 0
                    },
                    "password": {
                        "type": "string",
                        "description": "Password del usuario.",
                        "maxLength": 100,
                        "minLength": 0
                    },
                    "roles": {
                        "type": "string",
                        "description": "Rol del usuario.",
                        "maxLength": 50,
                        "minLength": 0
                    },
                    "fullName": {
                        "type": "string",
                        "description": "Nombre completo del usuario.",
                        "maxLength": 100,
                        "minLength": 0
                    },
                    "sales": {
                        "type": "integer",
                        "format": "int32",
                        "description": "Ventas totales."
                    },
                    "earnings": {
                        "type": "number",
                        "format": "double",
                        "description": "Ganancias totales."
                    }
                },
                "required": [
                    "fullName",
                    "password",
                    "roles",
                    "username"
                ]
            },
            "Supplier": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Identificador único del proveedor."
                    },
                    "name": {
                        "type": "string",
                        "description": "Nombre del proveedor.",
                        "maxLength": 50,
                        "minLength": 0
                    }
                },
                "required": [
                    "name"
                ]
            },
            "Category": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Identificador único de la categoría."
                    },
                    "name": {
                        "type": "string",
                        "description": "Nombre de la categoría.",
                        "maxLength": 50,
                        "minLength": 0
                    },
                    "description": {
                        "type": "string",
                        "description": "Descripción de la categoría.",
                        "maxLength": 255,
                        "minLength": 0
                    }
                },
                "required": [
                    "description",
                    "name"
                ]
            },
            "Product": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Identificador único del producto."
                    },
                    "code": {
                        "type": "string",
                        "description": "Código del producto.",
                        "maxLength": 50,
                        "minLength": 0
                    },
                    "name": {
                        "type": "string",
                        "description": "Nombre del producto.",
                        "maxLength": 50,
                        "minLength": 0
                    },
                    "description": {
                        "type": "string",
                        "description": "Descripción del producto.",
                        "maxLength": 255,
                        "minLength": 0
                    },
                    "category": {
                        "$ref": "#/components/schemas/Category",
                        "description": "Categoría a la que pertenece el producto."
                    },
                    "supplier": {
                        "$ref": "#/components/schemas/Supplier",
                        "description": "Proveedor del producto."
                    },
